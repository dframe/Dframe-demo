{
    "name": "dframe/dframe-demo",
    "description": "It is a simple demo website based on Dframe Framework",
    "license": "MIT",
    "keywords": [
        "dframe",
        "demo",
        "framework",
        "website",
        "application",
        "microservices"
    ],
    "homepage": "https://dframeframework.com",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "app/",
            "Mail\\": "modules/Mail/src"
        }
    },
    "require": {
        "dframe/dframe": "^4.0",
        "dframe/database": "^2.0",
        "smarty/smarty": "^3.1",
        "dframe/mymail": "^1.1",
        "ext-pdo": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Dframe\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "php vendor/bin/phpunit"
    }
}
