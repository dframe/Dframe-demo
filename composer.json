{
    "name": "dframe/dframe-demo",
    "description": "It is a simple demo website based on Dframeframework",
    "license": "MIT",
    "keywords": [
        "dframe",
        "demo",
        "framework",
        "website",
        "application",
        "blog"
    ],
    "homepage": "https://dframeframework.com",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "app/"
        }
    },
    "require": {
        "dframe/dframe": "dev-beta-modular",
        "dframe/database": "^1.3",
        "smarty/smarty": "^3.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Dframe\\tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "php vendor/bin/phpunit"
    }
}